<div class="col-md-12" style="position:fixed; z-index: 1;">
  <div class="col-md-12 page-id-header">
     <h3>2015 PLAYOFFS PLAYER PROFILE</h3>
  </div>
  <div class="col-md-12 page-sub-nav"> 
    <h4>Finals <%= @po_playerdata[0]["DISPLAY_FIRST_LAST"] %></h4>
  </div>
  <ul class="nav navbar-nav">
  	  <li><a href="/players/<%= @po_playerdata[0]["PERSON_ID"] %>" role="button" id="all">All</a></li>	
      <% if @rd_player_dashboard_totals.body["resultSets"][0]["rowSet"] != [] %>
	      <li><a href="/players/round1/<%= @po_playerdata[0]["PERSON_ID"] %>" role="button" id="round1">Round 1</a></li>
      <% end %>
      <% if @sf_player_dashboard_totals.body["resultSets"][0]["rowSet"] != [] %>
	      <li><a href="/players/semis/<%= @po_playerdata[0]["PERSON_ID"] %>" role="button" id="semis">Semis</a></li>
      <% end %>
      <% if @cf_player_dashboard_totals.body["resultSets"][0]["rowSet"] != [] %>
	      <li><a href="/players/conf_finals/<%= @po_playerdata[0]["PERSON_ID"] %>" role="button" id="conf">Conf Finals</a></li>
      <% end %> 
      <% if @f_player_dashboard_totals.body["resultSets"][0]["rowSet"] != [] %>
	      <li><a href="/players/finals/<%= @po_playerdata[0]["PERSON_ID"] %>" role="button" id="finals">Finals</a></li>
	  <% end %>
    </ul>
</div>
<!-- Main Pane -->
<div class="main">
    <!-- Player Info -->
    <div class="col-md-3 player-page-profile">
      <div style="width: 100%">
      <img src="http://stats.nba.com/media/players/230x185/<%= @po_playerdata[0]["PERSON_ID"] %>.png" width="200px">
      </div>
      <div class="player-page-name">
        <p><%= @po_playerdata[0]["DISPLAY_FIRST_LAST"] %></p>
      </div>
<!-- Condition display data only if team has played that round -->
<% if @f_player_dashboard_totals.body["resultSets"][0]["rowSet"] != [] %>
      <h4><%= (@po_PlayerTotalsdata[0]["PTS"]/ @po_PlayerTotalsdata[0]["GP"].to_f).round(1) %> PTS / <%= (@po_PlayerTotalsdata[0]["REB"]/ @po_PlayerTotalsdata[0]["GP"].to_f).round(1) %> REB / <%= (@po_PlayerTotalsdata[0]["AST"]/ @po_PlayerTotalsdata[0]["GP"].to_f).round(1) %> AST</h4> 
    </div> 
<!-- Message when the condition isn't met -->    
<% else %>
<h4> no data </h4>
<% end %> 
</div><!-- END Main Pane -->