<%= render 'shared/team_header' %>
<!-- Main Pane -->
<div class="main">
	<div class="page-sub-nav col-md-12">	 
	  	<h4><%= @team.team_city %> <%= @team.team_name %></h4> 	 	
	</div>
	<div class="row">
    <div class="col-md-4 team-page-profile">
      
      	<% if @team.team_city == "Atlanta" %>
		  <img src="http://stats.nba.com/media/img/teams/logos/atl_logo.svg">
   	  
   	  <% elsif @team.team_city == "Boston" %>
		  <img src="http://stats.nba.com/media/img/teams/logos/bos_logo.svg">
   	  
   	  <% elsif @team.team_city == "Brooklyn" %>
		  <img src="http://stats.nba.com/media/img/teams/logos/bkn_logo.svg">
   	  
   	  <% elsif @team.team_city == "Chicago" %>
